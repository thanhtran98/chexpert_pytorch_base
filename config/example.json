{   
    "train_csv": "/Path/to/train.csv",
    "dev_csv": "/Path/to/dev.csv",
    "backbone": "densenet",
    "id": "121",
    "split_output": true,
    "modify_gp": true,
    "mix_precision": true,
    "pretrained": true,
    "full_classes": false,
    "conditional_training": false,
    "smooth_mode": "pos",
    "lr": 1e-4,
    "batch_size": 16,
    "width": 512,
    "height": 512,
    "long_side": 512,
    "fix_ratio": true,
    "pixel_mean": 128.0,
    "pixel_std": 64.0,
    "use_pixel_std": true,
    "use_equalizeHist": true,
    "use_transforms_type": "Aug",
    "gaussian_blur": 3,
    "border_pad": "pixel_mean",
    "num_classes": [1,1,1,1,1],
    "batch_weight": true,
    "enhance_index": [2,6],
    "enhance_times": 1,
    "pos_weight": [1,1,1,1,1],
    "global_pool": "PCAM",
    "fc_bn": false,
    "lse_gamma": 0.5,
    "fc_drop": 0,
    "optimizer": "Adam",
    "momentum": 0.9,
    "weight_decay": 0.0,
    "epochs": 5,
    "iter_log": 200,
    "log_dir": "DenseNet121_2601",
    "ckp_path": "./"
}